.splash, .splash2
  min-height 680px;
  overflow hidden;
  position relative;
  width 100%;
  padding-bottom 10px

.splash2
  min-height 220px;

.splash .splash-image-container, .splash2 .splash-image-container
  min-height 680px;
  width 100%;
  position absolute;
  left 0;
  right 0;
  top 0;
  bottom 0;
  margin-bottom 50px;

.splash2 .splash-image-container
  min-height 520px;

.splash .splash-image,.splash2 .splash-image
  min-height 695px;
  width 110%;
  position absolute;
  left 0;
  right 0;
  top 0;
  bottom 0;

  /*z-index  1;*/
  display  block;
  vertical-align  middle;
  background-position center center;
  background-repeat repeat;
  -webkit-filter: blur(3px);
  -moz-filter: blur(3px);
  -o-filter: blur(3px);
  -ms-filter: blur(3px);
  filter: blur(3px);
  background-color #000000;
  margin: -10px -10px -10px -10px;

