.list-group-review
  display inline-table;
  padding 20px 20px 10px 20px;
  min-width: 280px;
  /*margin-right 10px;*/
  /*margin-bottom 10px;*/
  background #FFFFFF;
  border solid 1px #EEEEEE;

.single-review
  display inline-table;
  /*width 22em;*/
  padding-top 20px;
  padding-bottom 20px;
  min-width: 280px;
  background #FFFFFF;
  /*border solid 1px #EEEEEE;*/

.profile-picture
  width: 40px;
  height 40px;
  margin-top: 8px;

.profile-picture div
  border-radius: 20px;
  -webkit-border-radius: 17.5px;
  -moz-border-radius: 17.5px;
  padding-top 3px
  width: 35px;
  height: 35px;
  background-size cover

.user-avatar
  float left;
  width:50px

.image-holder, .profile-image-holder
  float left
  margin-right 10px
  width: 70px;
  height: 70px;
  background-size: cover;
  overflow hidden

.profile-image-holder
  width: 150px;
  height: 150px;

.user-information
  font-size 21px;
  float: left;

.user-information .date
  font-size 15px

.user-information .school-name
  font-size 11px

.default-user-picture, .profile-img
  position: absolute;
  top: 0;

.user-picture
  position: relative;
  width: 70px;
  height: 70px;
  float: left;
  margin-right: 10px;

.review-content
  height: 75px;
  overflow hidden
  -webkit-box-shadow: inset 0px -10px 15px -2px #F0F0F0;

#load-more-reviews,
#btn-review{
  display: inline-block;
  margin-right: 10px;
  vertical-align: top;
}

.read-review
  padding-top 20px
  height auto
  /*overflow hidden*/
  text-align left
  -webkit-box-shadow: none


/////////////////////////////////////////////////////////////////////////////////////
//SCHOOL PAGE -> Rating Card
/////////////////////////////////////////////////////////////////////////////////////

#rating
  margin-left 0;
  margin-right 0;
  width auto;

.school-rating-card
  /*text-align center;*/
  border solid 1px #cccccc;
  background-color #f9f9f9;
  margin-top 10px;
  padding 15px 10px 15px 10px;
  /*width 300px*/
  /*height: 140px;*/

.center
  width 100px
  margin auto
  padding-left 10px

.school-breakdown-card
  border solid 1px #cccccc;
  background-color #f9f9f9;
  margin-top 10px;
  padding 15px 10px 15px 10px;

.school-breakdown-card .criteria-name ul li
  line-height 16px
  font-weight 300;
  font-size 17px;

@media (max-width: 568px)
  .school-breakdown-card .criteria-name ul li
    line-height 16px
    font-weight 300;
    font-size 14px;

.school-rating
  font-size 60px;

.inline
  display: inline;

.spacious
  padding-right 2px;

.star-rating-row p
  margin 0 0 8px;
  line-height 23px;

/////////////////////////////////////////////////////////////////////////////////////
//Schoolpage helpfuls
/////////////////////////////////////////////////////////////////////////////////////
.hasHF
  color:#337ab7

.helpful-info
  line-height 30px;
/////////////////////////////////////////////////////////////////////////////////////
//WRITE REVIEW
/////////////////////////////////////////////////////////////////////////////////////

.criteria
  font-size 20px;
  padding 40px 0 10px 0

.review-criteria-score
  font-size 17px;
  text-align right;
  font-weight bold;

.no-top-margin
  margin-top 0;
  margin-left 0;

.calendar
  padding-top 10px;

.user-box
  display flex
  padding: 15px 10px 15px 10px
  background-color #FFF

.school-image-preview
  height 300px
  background-size cover

