//COLORS
china-red = #AA381E;
deep-blue = #2A303C;
china-green = #87D37C;

/** {*/
  /*-webkit-border-radius: 0 !important;*/
  /*-moz-border-radius: 0 !important;*/
  /*border-radius: 0 !important;*/
/*}*/

body
  padding: 0px;
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
  background-color #f4f3f0

a
  color: inherit

a:hover
  text-decoration none;


h1
  font-size 3.5em

.extra-link
  padding-top:20px;

.flat-body
  background-color: #EEE;

header
  width: 100%;
  height: 60px;
  background-color: china-green;

footer
  border-top solid 1px #555555
  margin-top 50px
  width: 100%;
  height: 200px;
  background-color: deep-blue;

.lift-content
  margin-top 100px;
  background-color white;

.search-navigation
  background-color: #FFF;
  border: solid 1px #CCC;

.search-navigation-search-input
  width 50%

.school-image-container
  overflow hidden
  height: 400px;
  border-top: solid 10px #000;
  border-bottom: solid 10px #000;
  background-position center center;

.featured-school
  overflow hidden
  height 500px
  /*border-bottom solid 10px black*/
  background-position center center;

.featured-school-item
  height:500px;
  background-position center center;
  background-size cover

.list-group-item
  display inline-block;

a h2.list-group-item-heading,a h2.list-group-item-heading:hover
  color: black;
  font-size: 3em;

.school-list-wrapper
  padding-left 0px;
  padding-right 0px;

.list-group-item.school-list
  margin 10px 0;
  border: solid 1px #EEE;
  padding 0px;
  padding-bottom 10px;

.list-group-item.school-list:hover
  /*border-top: solid 5px #080808*/
  box-shadow: 2px 2px 6px #cccccc;
  border-left solid 2px #EEE;


.school-list-text
  padding-top 5px
  display block;

.school-list-information a h2, .school-list-information a h2:hover
  text-decoration none

.school-list-image
  padding-top 20px;
  padding-left 20px;
  overflow hidden

.custom-jumbotron
  padding-top 0;
  padding-bottom: 0;
  margin-bottom -20px

.navbar-custom
  border: 0px;
  /*border-bottom solid 1px #FFFFFF;*/
  background-color: #FFF;
  color:#FFF;
  margin-bottom: 0px;

.school-list-information
  padding-top 15px;

#ex1Slider .slider-selection
  background: #BABABA;

.criteria
    font-size 20px;

.rating
  font-size: 30px;
  color #e38d13
  /*color #4cae4c*/

#rating
  font-size: 45px;

.list-group-review
  display inline-table;
  /*width 22em;*/
  padding 20px;
  margin-right 10px;
  margin-bottom 10px;
  background #FFFFFF;
  border solid 1px #EEEEEE;

.profile-picture
  width: 40px;
  height 40px;
  margin-top: 4px;

.profile-picture div
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  padding-top 3px
  width: 40px;
  height 40px;
  background-size cover

.user-avatar
  float left;
  width:50px

.image-holder, .profile-image-holder
  float left
  margin-right 10px
  width: 70px;
  height: 70px;
  background-size: cover;
  overflow hidden

.profile-image-holder
  width: 150px;
  height: 150px;

.user-information
  font-size 23px;
  float-left

.user-information .date
  font-size 20px

.container h3
  text-transform uppercase
  font-size 18px

footer .container
  color:#CCCCCC

footer .container ul
  list-style:none;
  padding-left:0;

footer .container ul li
  padding-bottom:15px;

.search-title
  padding:0 0 0 15px;
  color:#000;

#mission
  padding:10px 0 0 0;
  font-size 10px;